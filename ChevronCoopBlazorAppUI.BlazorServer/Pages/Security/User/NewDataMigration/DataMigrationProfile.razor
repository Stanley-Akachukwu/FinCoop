                            <h3 class="text-gray-900 sm:text-sm rounded-lg  block w-full    dark:text-white dark:focus:ring-primary-500">@CustomerBankDetail.Branch</h3>

                        </div>
                        @* <div class="col-span-6 sm:col-span-3">
                                <label for="state"
