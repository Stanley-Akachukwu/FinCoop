                    <DropDownListFieldSettings Value="Code" Text="Name"></DropDownListFieldSettings>
                </SfDropDownList>
            </div>


            <div class="col-span-6 sm:col-full">
                <button type="button" data-modal-toggle="add-product-modal" class="inline-flex items-center py-2 px-3 text-sm font-medium text-center text-CEMCS-Blue-100 rounded-lg bg-white hover:bg-CEMCS-Blue-100 hover:text-white ring-2 ring-primary-300 sm:ml-auto dark:bg-primary-600 dark:hover:bg-primary-700 dark:focus:ring-primary-800" @onclick="OnAddNewNextOfKin">
                    <svg class="mr-2 -ml-1 w-6 h-6" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg">
                        <path fill-rule="evenodd" d="M10 5a1 1 0 011 1v3h3a1 1 0 110 2h-3v3a1 1 0 11-2 0v-3H6a1 1 0 110-2h3V6a1 1 0 011-1z" clip-rule="evenodd"></path>
                    </svg>
                    Add new Next of Kin
                </button>
            </div>
            <div>
                <SfGrid DataSource=@NextOfKins TValue="NextOfKinData" Width="570px" ID="Grid">


                    <GridColumns>
                        <GridColumn Field=@nameof(NextOfKinData.Serial) TextAlign="TextAlign.Center" HeaderText="S/N" Width="50px"></GridColumn>
                        <GridColumn Field=@nameof(NextOfKinData.FirstName) HeaderText="First Name" Width="130px"></GridColumn>
                        <GridColumn Field=@nameof(NextOfKinData.LastName) HeaderText="Last Name" Width="130px"></GridColumn>
                        <GridColumn Field=@nameof(NextOfKinData.PhoneNumber) HeaderText="Phone Number" Width="130px"></GridColumn>
                        <GridColumn Field=@nameof(NextOfKinData.Address) HeaderText="Address" Width="130px"></GridColumn>
                        <GridColumn Field=@nameof(NextOfKinData.Relationship) HeaderText="Relationship" Width="130px"></GridColumn>
                    </GridColumns>
                </SfGrid>
            </div>
            <div class="col-span-6 sm:col-full">
                <button class="text-white bg-CEMCS-Blue-100 hover:bg-CEMCS-Blue-100 focus:ring-4 focus:ring-primary-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center dark:bg-primary-600 dark:hover:bg-primary-700 dark:focus:ring-primary-800"
